<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GreenEarth" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:navigationBarColor">#0B3727</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#1D1F1B</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="rebuke" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
        <item name="fontFamily">@font/titanoneregular</item>
    </style>

    <style name="cool">
        <item name="android:background">#5E501B</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:drawableLeft">@drawable/jj</item>
    </style>

    <style name="Theme.GreenEarth.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.GreenEarth.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.GreenEarth.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



</resources>